{
  "source": "./lib",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./test",
          "includes": [
            "\\.js$"
          ]
        },
        "manual": {
          "index": "./manual/index.md",
          "asset": "./manual/asset",
          "files": [
            "./manual/getting-started.md"
          ]
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "decorators": true,
        "classProperties": true
      }
    }
  ]
}
